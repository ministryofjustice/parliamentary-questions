<h4><% if @pq.seen_by_finance %></h4>
	<p><i class="fa fa-check-circle text-success"></i> PQ viewed</p>
<% else %>
    <p><i class="fa fa-times-circle text-danger"></i> PQ not viewed</p>
<% end %>
<div class="checkbox">
    <%= f.label(:seen_by_finance) %>
    <%= f.check_box(:seen_by_finance) %>
</div>
<div class="checkbox">
	<%= f.label(:finance_interest, 'Interest in PQ') %>
	<%= f.check_box(:finance_interest) %>
</div>